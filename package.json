{
  "name": "@openagenda/verror",
  "description": "richer JavaScript errors",
  "version": "3.1.1",
  "license": "MIT",
  "keywords": [
    "error",
    "errors",
    "err",
    "exception",
    "exceptions",
    "custom"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenAgenda/verror.git"
  },
  "files": [
    "esm",
    "lib"
  ],
  "main": "lib/index.js",
  "jsnext:main": "esm/esm.js",
  "module": "esm/esm.js",
  "sideEffects": false,
  "scripts": {
    "prepack": "yarn clean && yarn build",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "babel --copy-files src -d esm -s --env-name esm",
    "build:cjs": "babel --copy-files src -d lib -s",
    "clean": "rimraf esm lib",
    "test": "jest"
  },
  "dependencies": {
    "assertion-error": "^1.1.0",
    "depd": "^2.0.0",
    "inherits": "^2.0.4",
    "sprintf-js": "^1.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@openagenda/babel-preset": "^1.5.0",
    "jest": "^26.1.0",
    "rimraf": "^3.0.1"
  }
}
